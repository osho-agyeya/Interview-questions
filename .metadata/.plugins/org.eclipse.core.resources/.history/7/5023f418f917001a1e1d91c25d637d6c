package leetcode;

public class First_Bad_Version {

	public int firstBadVersion(int n) {
        int left = 1;
    int right = n;
    while (left < right) {
        int mid = left + (right - left) / 2;
			/* uncomment this in actual code
			 * if (isBadVersion(mid)) { right = mid; } else { left = mid + 1; }
			 */
    }
    return left;
    }
	
}
