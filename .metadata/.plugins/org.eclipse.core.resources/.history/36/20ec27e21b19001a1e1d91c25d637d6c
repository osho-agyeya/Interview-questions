//done

package interviewbit;

public class Amazing_Subarrays {
public int solve(String A) {
        
        String vow = "aeiouAEIOU";
        int count = 0;
        int s = A.length();
        for(int i=0; i<A.length(); i++){
            if(vow.indexOf(A.charAt(i)) >= 0){
                count += s-i;
            }
        }
        return count % 10003;
    }
}
