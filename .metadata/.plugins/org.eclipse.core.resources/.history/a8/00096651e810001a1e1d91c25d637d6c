package interviewbit;

public class Repeating_Sub_Sequence {
	
	public int anytwo(String A) {
        int len=A.length();
        if(len<=2)
        return 0;
        
        String ans=LCS(A,A);
        if(ans.length()<2)
        return 0;
        return 1;
        
    }
    private String LCS(String s1,String s2){
        String ans="";
        int last_ind=1;
        for(int i=0;i<s1.length();i++)
        {
            char ch1=s1.charAt(i);
            int ind=s2.indexOf(ch1,last_ind);
            //System.out.print(ind+" ");
            if(ind!=-1 && ind!=i)
            {ans+=ch1;
            last_ind=ind;}
        }
        return ans;
    }
    
    

}
