package interviewbit;

public class Largest_Coprime_Divisor {
	
	public int cpFact(int A, int B) {
        while (gcd(A,B) != 1) {
            A = A/gcd(A,B);
        }
        return A;
    }
    public int gcd(int A, int B) {
        if (B == 0) {
            return A;
        } else {
            return gcd(B, A%B);
        }
    }

}
