package leetcode;

public class Find_Minimum_in_Rotated_Sorted_Array {
	
	public int findMin(int[] nums) {
        
		int left = 0,  right = nums.length - 1;
    while(left < right) {
            
        int mid = (left + right)/2;
        if(nums[mid] > nums[right])
            left = mid + 1;nd
        else
            right = mid;
    }
    
    return nums[left];
		
    }

}
