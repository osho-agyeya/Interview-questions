package interviewbit;

import java.util.Arrays;

public class Wave_Array {
	
	 public int[] wave(int[] a) {
        Arrays.sort(a);
        for(int i=1;i<a.length;i+=2){
            int temp=a[i-1];
            a[i-1]=a[i];
            a[i]=temp;
        }
        return a;
    }

}
