package interviewbit;

public class Matrix_Chain_Multiplication {
	
	 public int solve(int[] arr) {
		 int temp[][] = new int[arr.length-1][arr.length-1];
	        int q = 0;
	        for(int l=2; l <= arr.length-1; l++){
	            for(int i=0; i < arr.length - l; i++){
	                int j = i + l-1;
	                temp[i][j] = 1000000;
	                for(int k=i+1; k <= j; k++){
	                    q = temp[i][k] + temp[k][j+1] + arr[i]*arr[k]*arr[j+1];
	                    if(q < temp[i][j]){
	                        temp[i][j] = q;
	                    }
	                }
	            }
	        }
	        return temp[0][arr.length-2];
	    }
	 
	 
	 public static void main(String[] args) {
		int result=new Matrix_Chain_Multiplication().solve(new int[]{2, 3, 6, 4, 5});
	}

}
