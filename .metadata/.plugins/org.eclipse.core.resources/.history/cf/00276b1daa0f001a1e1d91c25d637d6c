package leetcode;

public class Reverse_String {

	public void reverseString(char[] word) {
        //char[] word = s.toCharArray();
        int i = 0;
        int j = word.length - 1;
        while (i < j) {
            char temp = word[i];
            word[i] = word[j];
            word[j] = temp;
            i++;
            j--;
        }
    }
	
}
