package interviewbit;

public class INVERSIONS {
	
	 public int countInversions(int[] a) {
        int c=0;
        int l=a.length;
        for(int i=0;i<l-1;i++){
            for(int j=i+1;j<l;j++){
                if(a[i]>a[j]){
                    c++;
                }
            }
        }
        return c;
    }

}
