package leetcode;

import main_solution.ListNode;

public class Swap_Nodes_in_pairs {
	
	public ListNode swapPairs(ListNode head) {
        if ((head == null)||(head.next == null))
            return head;
        ListNode n = head.next;
        head.next = swapPairs(head.next.next);
        n.next = head;
        return n;
    }

}
