package ctci;

public class Remove_Dups {

	 Set<Integer> set = new HashSet<>();
	    ListNode pre = null;
	    ListNode fakeHead = head;
	    while(fakeHead != null){
	        if(!set.add(fakeHead.val)){
	            pre.next = fakeHead.next;
	        } else{
	            pre = fakeHead;
	        }
	        fakeHead = fakeHead.next;
	    }
	    return head;
	
	
}
