//doe

package interviewbit;

public class Max_Sum_Without_Adjacent_Elements {
	
	public int adjacent(int[][] A) {
        int l= A[0].length;
        int[] a = new int[l+1];
        a[1]=Math.max(A[0][0],A[1][0]);
        for(int i=2 ;i<l+1;i++){
            a[i]=Math.max(a[i-1],a[i-2]+Math.max(A[0][i-1],A[1][i-1]));
        }
        return a[l];
    }

}
