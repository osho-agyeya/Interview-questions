package leetcode;

import java.util.Arrays;

public class Longest_Increasing_Subsequence {
	
	public int lengthOfLIS(int[] nums) {
        int[] dp = new int[nums.length];
       int len = 0;

       for(int x : nums) {
           int i = Arrays.binarySearch(dp, 0, len, x);
           System.out.println(i);
           if(i < 0) i = -(i + 1);
           dp[i] = x;
           if(i == len) len++;
       }

       return len;
   }

}
