//doe

package leetcode;

public class Rotate_Image {
	
	public void rotate(int[][] matrix) {
	       int len = matrix.length;
	        for(int i = 0; i < len / 2; i++) {
	            int[] tmp = matrix[i];
	            matrix[i] = matrix[len - 1 - i];
	            matrix[len - 1 - i] = tmp;
	        }
	        
	        for(int i = 0; i < len; i++) {
	            for(int j = 0; j < i; j++) {
	                int tmp = matrix[i][j];
	                matrix[i][j] = matrix[j][i];
	                matrix[j][i] = tmp;
	            }
	        } 
	    }

}
